package gradingActivity;

public class Demo4BodyBag {

	public static void main(String[] args) {
		Student s1 = new Student("Johnny", 20, 3, 100, 0, 0, 100);
		Student s2 = new Student("Suzy", 20, 1, 100, 100, 100, 100);
		Student s3 = new Student("Joey", 20, 10, 100, 100, 100, 100);
		BodyBag theBag = new BodyBag(10);
		theBag.insert(s1);
		theBag.insert(s2);
		theBag.insert(s3);
		theBag.display();
		System.out.println("-------Looking for Suzy-------");
		System.out.println(theBag.findByName("Suzy").getGrades().getLetterGrade());
	}

}
