package sampleCode1;

public class FacultyBag {

	private Faculty[] facultyArray;
		private int nElems;
	
		public FacultyBag(int maxSize) {
			facultyArray = new Faculty[maxSize];
			nElems = 0;
		}
		
		public void insert(Faculty f){
			facultyArray[nElems++] = f;
		}
		
		public void display(){
			for(int i = 0; i < nElems; i++){
				System.out.println(facultyArray[i]);
			}
			System.out.println();
		}
	

}
