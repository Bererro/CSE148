package p3;

public class Demo {

	public static void main(String[] args) {
		Student s1 = new Student("John", 4.0);
		Faculty f1 = new Faculty("Chen", 10);
		Course c1 = new Course("CSE148", s1, f1);
		System.out.println(c1.getFaculty());
		c1.getFaculty().setName("John Doe");
		System.out.println(c1.getFaculty());
		}

}
