package stringBuilder;

public class Demo1 {

	public static void main(String[] args) {
		StringBuilder sb1 = new StringBuilder();
		sb1.append("Adam");
		StringBuilder sb2 = sb1;
		sb1.append("Billy");
		System.out.println(sb1 == sb2);
		
		sb1.delete(0, 1);
		System.out.println(sb1.toString());
		
	}

}
