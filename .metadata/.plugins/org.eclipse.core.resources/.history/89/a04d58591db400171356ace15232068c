package ducks;

public class Demo {

	public static void main(String[] args) {
		MallardDuck mallardDuck1 = new MallardDuck();
		mallardDuck1.quack();
		mallardDuck1.swim();
		mallardDuck1.display();
		
		RedHeadDuck redheadDuck = new RedHeadDuck();
		redheadDuck.quack();
		redheadDuck.swim();
		redheadDuck.display();
	}

}
